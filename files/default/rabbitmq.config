[
 {rabbit, [
  {tcp_listeners, []},
  {ssl_listeners, [5671]},
  {ssl_options, [
    {cacertfile,"/etc/rabbitmq/ssl/propel_host.crt"},
    {certfile,"/etc/rabbitmq/ssl/propel_host.crt"},
    {keyfile,"/etc/rabbitmq/ssl/propel_host.key.rsa"},
    {verify,verify_none}]}
 ]},
 { rabbitmq_management, [
  {listener, [
    {port, 15672},
    {ssl, true},
    {ssl_opts, [
      {cacertfile,"/etc/rabbitmq/ssl/propel_host.crt"},
        {certfile,"/etc/rabbitmq/ssl/propel_host.crt"},
        {keyfile,"/etc/rabbitmq/ssl/propel_host.key.rsa"}
     ]}
   ]}
 ]}
].
 
